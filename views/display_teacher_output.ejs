    <%include partials/header.ejs%> 
    <br>
    <div class="navbar">   
            <a href="/home">Home</a>
            <div class="dropdown">
              <button class="dropbtn">Department 
                <i class="fa fa-caret-down"></i>
              </button>
              <div class="dropdown-content" >
                <% for(var i=0; i < Departments.length; i++){%>
                <div class="row">
                  <div class="column" >
                    <h3><%=Departments[i]%></h3>
