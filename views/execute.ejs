<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
    <form action="" id="submitto" method="POST">

        <button type="submit" class="but" onclick="check('4')" id="4" >RESET DUTIES</button>   <br><br> <br>
        <button type="submit" class="but" onclick="check('13')" id="13">DELETE ALLOTED TEACHERS</button> <br><br> <br>
        <button type="submit" class="but" onclick="check('10')" id="10">DISPLAY_SELECTED TEACHERS</button> <br><br> <br>
        <button type="submit" class="but" onclick="check('14')" id="14">UT_algo</button> <br><br> <br>
        <button type="submit" class="but" onclick="check('16')" id="16">ALLOT UT SELECTED TEACHERS</button> <br><br> <br>
        <button type="submit" class="but" onclick="check('15')" id="15">SEM_algo</button> <br><br> <br>
        <button type="submit" class="but" onclick="check('17')" id="17">ALLOT SEM REGULAR SELECTED TEACHERS</button> <br><br> <br>
        <button type="submit" class="but" onclick="check('18')" id="18">KT_algo</button> <br><br> <br>
        <button type="submit" class="but" onclick="check('19')" id="19">ALLOT SEM KT SELECTED TEACHERS</button> <br><br> <br>
    </form>

    <script>
      function check(eve)
      {
          let link;
        switch(eve)
        {
            case '4': link="/sid_reset" ; break;
            case '10': link="/sid_selected";break;
            case '13': link="/delete_selected_teachers";break;
            case '14':link="/ut_algo";break;
            case '15':link="/sem_algo";break;
            case '16':link="/allot_ut_selected";break;
            case '17':link='/allot_sem_selected';break;
            case '18':link='/kt_algo';break;
            case '19':link='/allot_sem_kt_selected';break;    
        }

        document.getElementById("submitto").setAttribute("action",link);
           
      
    }  
        
    </script>
</body>
<link rel="stylesheet" href="../assets/css/perform.css">

      

</html>